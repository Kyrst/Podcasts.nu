<IfModule mod_rewrite.c>
	Options +FollowSymLinks
	RewriteEngine On

	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteRule ^ index.php [L]

	Redirect 301 /poddar/co-hannah-och-amanda /poddar/fredagspodden-med-hannah-och-amanda
	Redirect 301 /avsnitt/co-hannah-och-amanda /avsnitt/fredagspodden-med-hannah-och-amanda
	Redirect 301 /avsnitt/co-hannah-och-amanda/(.*).htm /avsnitt/fredagspodden-med-hannah-och-amanda/$1
</IfModule>