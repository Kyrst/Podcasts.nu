<IfModule mod_rewrite.c>
	Options +FollowSymLinks
	RewriteEngine On

	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteRule ^ index.php [L]

	RewriteRule ^poddar/co-hannah-och-amanda/$ /poddar/fredagspodden-med-hannah-och-amanda [L,NC,R=301]
	RewriteRule ^avsnitt/co-hannah-och-amanda/$ /avsnitt/fredagspodden-med-hannah-och-amanda [L,NC,R=301]
	RewriteRule ^avsnitt/co-hannah-och-amanda/(.*).htm$ /avsnitt/fredagspodden-med-hannah-och-amanda/$1 [L,NC,R=301]
</IfModule>